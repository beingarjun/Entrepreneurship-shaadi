{
  "version": 2,
  "name": "entrepreneur-shaadi",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next",
      "config": {
        "projectSettings": {
          "framework": "nextjs"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "functions": {
    "apps/web/pages/api/**/*.ts": {
      "runtime": "@vercel/node"
    }
  }
}